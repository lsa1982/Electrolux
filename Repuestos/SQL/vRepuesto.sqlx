SELECT 
	SQL_CALC_FOUND_ROWS  
	al1.idRepuesto, 
	al1.idCategoria ,
	al1.codigo ,
	al1.repuesto, 
	al1.imagen, 
	al1.cantidad, 
	al1.valor,
	al2.grupo,
	al2.tipo,
	al3.categoria,
	al1.ultimaModificacion
FROM 
	ELX_rep_Repuesto al1,
	elx_rep_grupo al2,
	elx_core_categoria al3
where
	al1.idGrupo = al2.idGrupo and
	al1.idCategoria = al3.idCategoria
