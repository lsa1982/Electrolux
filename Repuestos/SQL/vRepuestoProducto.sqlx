Select 
	al1.idProducto,
	al1.idRepuesto,
	al2.codigo,
	al2.nombre,
	al3.categoria,
	al3.tipo,
	al4.linea,
	al5.marca
FROM 
	elx_rep_productoRepuesto al1,
	elx_core_producto al2,
	elx_core_categoria al3,
	elx_core_linea al4,
	elx_core_marca al5
WHERE
	al1.idProducto = al2.idProducto and
	al2.idCategoria = al3.idCategoria and
	al2.idLinea = al4.idLinea and
	al4.idLinea = al5.idMarca

