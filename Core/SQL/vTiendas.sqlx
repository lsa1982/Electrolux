Select 
	SQL_CALC_FOUND_ROWS
	al1.idTienda,
	al1.idCadena,
	al1.tienda,
	al1.status,
	al1.geo_x,
	al1.geo_y,
	al1.ultimaModificacion,
	al1.idCategoria,
	al2.categoria,
	al3.cadena,
	al1.codigoTienda
From
	elx_core_tienda al1,
	elx_core_categoria al2,
	elx_core_cadena al3
Where
	al1.idCategoria = al2.idCategoria and
	al1.idCadena = al3.idCadena